{
  "name": "jsux/designpatterns",
  "description": "Design patterns implementation in php",
  "authors": [
    {
      "name": "datikken",
      "email": "tikken23@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.3",
    "symfony/finder": "^5.2",
    "illuminate/database": "^8.25"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3",
    "symfony/var-dumper": "^5.2"
  },
  "scripts": {
    "codestyle:check": "./vendor/bin/phpcs -p -s -n --standard=ruleset.xml --ignore=vendor src",
    "codestyle:fix": "./vendor/bin/phpcbf --standard=ruleset.xml src/"
  },
  "autoload": {
    "psr-4": {
      "App\\": ""
    }
  }
}
